{
  "name": "travis-tweet-update",
  "version": "0.5.0",
  "description": "Tweet on successful build with link to website or link.",
  "homepage": "https://www.robertgabriel.ninja",
  "author": {
    "name": "Robert James Gabriel",
    "email": "robert_gabriel@outlook.com",
    "url": "https://www.robertgabriel.ninja"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "keywords": [
    "node",
    "jest",
    "travis",
    "twitter",
    "tweet"
  ],
  "devDependencies": {
    "coveralls": "^3.0.0",
    "eslint": "^5.4.0",
    "prettier": "^1.11.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-config-xo": "^0.24.2",
    "jest": "^23.5.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "camelcase": [
        2,
        {
          "properties": "never"
        }
      ],
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "repository": "RobertJGabriel/travis-tweet-update",
  "jest": {
    "testEnvironment": "node"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "twitter": "^1.7.1"
  }
}
